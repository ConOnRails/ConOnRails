<h1>Radio Assignment Audit Trail</h1>

<table>
  <tr>
    <th>Radio</th>
    <th>Volunteer</th>
    <th>State</th>
    <th>At</th>
  </tr>

<% @radio_assignment_audits.each do |a| %>
  <tr>
    <td><%= link_to a.radio.name, a %></td>
    <td><%= link_to a.volunteer.name, a.volunteer %></td>
    <td><%= a.state %></td>
    <td><%= a.created_at %></td>
  </tr>
<% end %>
</table>

